        <addForeignKeyConstraint baseColumnNames="clause_id_id"
                                 baseTableName="product_clauses"
                                 constraintName="fk_product_clauses_clause_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clauses"/>

    </changeSet>
</databaseChangeLog>
