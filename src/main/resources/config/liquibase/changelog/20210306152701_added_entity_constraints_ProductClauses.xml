<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ProductClauses.
    -->
    <changeSet id="20210306152701-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_id_id"
                                 baseTableName="product_clauses"
                                 constraintName="fk_product_clauses_product_id_id"
                                 referencedColumnNames="id"
